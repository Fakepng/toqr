{
  "name": "To QR",
  "version": "1.0.0",
  "description": "Convert url to QR code",
  "manifest_version": 3,
  "author": "Krit Kasemtewin",
  "action": {
    "default_popup": "index.html",
    "default_title": "To QR"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "96": "favicon-96x96.png"
  },
  "permissions": ["tabs", "activeTab"]
}
